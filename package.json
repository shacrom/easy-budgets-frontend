{
  "name": "easy-budgets-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:develop": "ng build --configuration develop",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "ng lint",
    "db:migration:new": "supabase migration new",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:diff": "supabase db diff",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --linked > src/types/supabase.types.ts",
    "db:link:develop": "supabase link --project-ref %SUPABASE_PROJECT_REF_DEVELOP%",
    "db:link:main": "supabase link --project-ref %SUPABASE_PROJECT_REF_MAIN%",
    "db:deploy:develop": "npm run db:link:develop && npm run db:push && npm run db:types",
    "db:deploy:main": "npm run db:link:main && npm run db:push && npm run db:types"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/cdk": "^20.2.12",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^20.2.12",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@standard-schema/spec": "^1.0.0",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/pdfmake": "^0.2.12",
    "material-design-icons": "^3.0.1",
    "pdfmake": "^0.2.20",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@analogjs/vitest-angular": "^1.22.5",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "typescript": "~5.9.3",
    "vitest": "^4.0.8"
  }
}
